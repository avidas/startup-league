extends ../layout

block content
  .page-header
    div
      h3 Leagues
      .pull-right
        a(href='/new_league') Create New League!
  .row
    .col-sm-6.col-sm-offset-3
      .panel.panel-default
        .panel-body
          ul.feed
            each val in ["Contrarians", "Austinites", "Hardware League"]
              li.feed-item
                span.name= val
